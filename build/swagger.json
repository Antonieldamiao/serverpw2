            "MessageSuccessUserValidate": {
                "type": "object",
                "properties": {
                    "MENSAGEM": {
                        "$ref": "#/components/schemas/MessageSuccessValidateUser"
                    }
                }
            },
            "MessageErrorUserList": {
                "type": "object",
                "properties": {
                    "MENSAGEM": {
                        "$ref": "#/components/schemas/MessageErrorListUser"
