                "type": "object",
                "properties": {
                    "MENSAGEM": {
                        "$ref": "#/components/schemas/MessageErrorUserAdd"
                    }
                }
            },
            "Contact": {
                "type": "object",
                "properties": {
                    "name": {
                        "$ref": "#/components/schemas/name"
                    },
                    "telephone": {
